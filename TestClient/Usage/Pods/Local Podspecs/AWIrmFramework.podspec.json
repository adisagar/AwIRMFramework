{
  "name": "AWIrmFramework",
  "platforms": {
    "ios": "8.0"
  },
  "summary": "This framework does all IRM operations",
  "requires_arc": true,
  "dependencies": {
    "ADAL": [
      "~> 2.2.2"
    ]
  },
  "version": "0.0.25",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "VMWare Airwatch": "adityaprasad@vmware.com"
  },
  "homepage": "https://github.com/adisagar/AwIRMFramework",
  "source": {
    "git": "'https://github.com/adisagar/AwIRMFramework.git'",
    "tag": "0.0.25"
  },
  "frameworks": [
    "CoreData",
    "MessageUI",
    "SystemConfiguration",
    "Security",
    "Foundation"
  ],
  "source_files": [
    "AWIrmFramework/**/*.{swift}",
    "AWIrmFramework/AWIrmFramework.h",
    "AWIrmFramework/Frameworks/MSRightsManagement.framework/Headers/*"
  ],
  "libraries": "resolv",
  "resources": [
    "AWIrmFramework/Frameworks/MSRightsManagementResources.bundle"
  ],
  "private_header_files": [
    "AWIrmFramework/Frameworks/MSRightsManagement.framework/Headers/MSRightsManagement.h",
    "AWIrmFramework/Frameworks/MSRightsManagement.framework/Headers/MSCustomProtection.h"
  ],
  "vendored_frameworks": "AWIrmFramework/Frameworks/MSRightsManagement.framework",
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC"
  }
}
